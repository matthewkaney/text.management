{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@core/*": ["core/*"],
      "@management/lang-tidal": ["packages/languages/tidal/ghci.ts"],
      "@management/*": ["node_modules/@management/*/src"]
    }
  },
  "exclude": [
    "app/web/*",
    "core/extensions/firebase/*",
    "packages/languages/supercollider/*"
  ]
}
